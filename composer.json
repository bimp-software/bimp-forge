{
    "name": "bimp/forge",
    "description": "Framework",
    "keywords": ["Framework","bimp forge","bimp software"],
    "type": "project",
    "homepage": "https://forge.bimp-software.cl/",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "dompdf/dompdf": "^3.1",
        "twig/twig": "^3.21",
        "endroid/qr-code": "^6.0",
        "cocur/slugify": "^4.6",
        "phpmailer/phpmailer": "^6.10",
        "verot/class.upload.php": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bimp\\Forge\\": "storage/src/forge/"
        },
        "files": [
            "app/functions/bimp_functions_custom.php",
            "app/functions/bimp_globals.php"
        ]
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php forge install"
        ]
    },
    "authors": [
        {
            "name": "bimp-software",
            "email": "bimp.software@gmail.com"
        }
    ],
    "bin": ["bin/install"],
    "minimum-stability": "stable",
    "prefer-stable": true
}
